################################################################################################################################################################
# Déplacer l'image dans la clé USB Ventoy #
###########################################
USB_VENTOY(){
sudo rm $VENTOY/Archlinux-custom.iso;
sudo mv $DIR/iso/Archlinux-custom.iso $VENTOY/archlinux-custom.iso;
}


TEST_(){
if [ ! -f $DIR/iso/Archlinux-custom.iso]; then
  RC=0
else
  RC=1
fi
# mv $VENTOY/archlinux-custom.iso;
}



